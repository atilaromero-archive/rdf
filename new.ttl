:Path
      rdf:type rdfs:Class;
      rdfs:label "Path".

:SubPath
      rdf:type rdfs:Class;
      rdfs:label "SubPath".

:components
      rdf:type owl:ObjectProperty;
      rdfs:domain :Path.

:pathSeparator
      rdf:type owl:ObjectProperty;
      rdfs:domain :Path.

:mydir
      rdf:type :Path;
      :pathSeparator "/";
      :components "C:/".
      

[ rdf:type :Path;
  :components "C:/" ]
